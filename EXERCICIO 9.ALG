Algoritmo "Exercicio 9"

Var
   a,b,c,d: inteiro



Inicio
      escreval ("digite o primeiro valor e aperte enter")
      leia (a)
      escreval ("digite o segundo valor e aperte enter")
      leia (b)
      escreval ("digite o terceito valor e aperte enter")
      leia (c)
      d<-a+c-b
      escreval ("o valor de ",a," mais ",c," menos ",b," é de:",d)


Fimalgoritmo