Algoritmo "exercicio 10"

Var
   a,c,l: inteiro
   z: real



Inicio
      escreval ("digite o primeiro valor")
      leia (a)
      escreval ("digite o segundo valor")
      leia (c)
      escreval ("digite o terceiro valor")
      leia (l)
      z<-(l+a*2)/c
      escreval ("o valor de: " ,l," mais " ,a," multiplicado por 2 e dividido por " ,c," é de: " ,z)


Fimalgoritmo